{
    "name": "DiSpogs v5",
    "description": "DiSpogs v5",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["https://*.discogs.com/release/*"],
          "js": ["content_script.js"],
          "run_at": "document_end"
        }
      ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "DiSpogs v5"
    },
    "host_permissions": [
        "https://accounts.spotify.com/*"
    ],
    "permissions": [
        "identity",
        "scripting"
    ]
}