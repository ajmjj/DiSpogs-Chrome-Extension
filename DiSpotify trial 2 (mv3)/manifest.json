{
  "manifest_version": 3,
  "name": "DiSpotify Trial v2",
  "description": "Discogs relase Spotify searcher",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://*.discogs.com/release/*"],
      "js": ["content_script.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["<all_urls>"],
  "permissions": ["scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
